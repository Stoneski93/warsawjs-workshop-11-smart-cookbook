{
  "name": "warsawjs-workshop-11-smart-cookbook",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://mbury.github.io/warsawjs-workshop-11-smart-cookbook",
  "proxy": {
    "/api": {
      "target": "http://localhost:3001",
      "pathRewrite": {
        "^/api/": "/"
      }
    }
  },
  "dependencies": {
    "normalizr": "^3.2.4",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.14",
    "redux": "^3.7.2",
    "redux-api-middleware": "^2.0.0-beta.3",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.75.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "api": "json-server --watch db.json --port 3001",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "gh-pages": "^1.0.0",
    "json-server": "^0.12.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.3.0"
  }
